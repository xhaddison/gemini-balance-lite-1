{
  "functions": {
    "api/v1/chat/completions.js": {
      "maxDuration": 60
    }
  },
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/",
      "destination": "/api"
    },
    {
      "source": "/api/v1/:path*",
      "destination": "/api/v1/:path*"
    },
    {
      "source": "/v1beta/(.*)",
      "destination": "/api/v1/chat/completions.js"
    }
  ]
}